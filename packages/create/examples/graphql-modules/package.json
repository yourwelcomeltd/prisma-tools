{
  "name": "graphql-modules-example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "description": "prisma and apollo server  example to create CUED system by one command",
  "scripts": {
    "start": "node dist/src",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "pal g && prisma generate",
    "db-dev": "prisma migrate dev",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index"
  },
  "dependencies": {
    "graphql-modules": "1.x",
    "@paljs/plugins": "8.1.0",
    "@prisma/client": "5.x",
    "apollo-server": "3.x",
    "graphql": "16.x",
    "reflect-metadata": "0.x"
  },
  "devDependencies": {
    "prisma": "5.x",
    "@types/node": "20.x",
    "@paljs/cli": "8.1.0",
    "prettier": "2.x",
    "ts-node": "10.x",
    "ts-node-dev": "2.x",
    "typescript": "5.x"
  }
}
