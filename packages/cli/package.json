{
  "name": "@paljs/cli",
  "description": "Pal cli will be your friend in developing nodejs full-stack projects to auto generate everything for you",
  "version": "7.0.2",
  "author": "Ahmed Elewa @AhmedElywa",
  "bin": {
    "pal": "./bin/run"
  },
  "bugs": "https://github.com/paljs/prisma-tools/issues",
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.5",
    "@paljs/create": "workspace:*",
    "@paljs/display": "workspace:*",
    "@paljs/generator": "workspace:*",
    "@paljs/schema": "workspace:*",
    "@paljs/utils": "workspace:*",
    "@paljs/types": "workspace:*",
    "cross-spawn": "^7.0.3",
    "enquirer": "^2.3.6",
    "fs-extra": "^10.0.1",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9",
    "@types/node": "20.12.12",
    "chai": "^4",
    "eslint": "^9.3.0",
    "eslint-config-oclif": "^5.2.0",
    "eslint-config-oclif-typescript": "^3.1.7",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "tslib": "^2.3.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://paljs.com",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "pal",
    "dirname": "paljs",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "repository": "paljs/prisma-tools",
  "scripts": {
    "build": "shx rm -rf dist && tsc -p tsconfig.build.json",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm lint",
    "prepack": "pnpm build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \\\"test/**/*.test.ts\\\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "3d5f31c5d025953338ec694c513894b6a4112e70"
}
